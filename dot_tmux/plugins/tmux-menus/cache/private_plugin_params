#!/bin/sh
# Autogenerated always sourced file - Fake bang path to help editors/linters

#  This is a cache of configs for the plugin.
#  By sourcing this instead of gathering it each time, tons of time
#  is saved when generating menus.

cfg_trigger_key="\\"
cfg_no_prefix=false

cfg_use_cache=true

cfg_mnu_loc_x="C"
cfg_mnu_loc_y="C"

cfg_format_title="'#[align=centre] #{@menu_name} '"
cfg_display_cmds=false

cfg_nav_next="-->"
cfg_nav_prev="<--"
cfg_nav_home="<=="

cfg_use_hint_overlays=true
cfg_show_key_hints=false

cfg_use_whiptail=false
cfg_alt_menu_handler=""

cfg_tmux_conf="/home/vscode/.tmux.conf"
cfg_log_file=""

cfg_use_notes=false

#
#  If tmux version has changed, the entie cache is invalidated
#
current_tmux_vers="3.3a"
current_tmux_vers_i="33"
current_tmux_vers_suffix="a"

#
# Get time stamps for repo and local file changes,
# This ensures cache is cleared any time the code has changed.
#
repo_last_changed="2025-06-20 13:12:50 +0200"
last_local_edit=""

#
# If menu is displayed shorter than this, assume it was due to not fitting
# the screen
#
t_minimal_display_time="0.5"
