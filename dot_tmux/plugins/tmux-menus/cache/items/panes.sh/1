tmux display-menu -T '#[align=centre] Handling Pane ' -x 'C' -y 'C' "Back to Main menu  <==" Left "run-shell '/home/vscode/.tmux/plugins/tmux-menus/items/main.sh'" "Split pane         -->" S "run-shell '/home/vscode/.tmux/plugins/tmux-menus/items/pane_split.sh'" "Layouts            -->" L "run-shell '/home/vscode/.tmux/plugins/tmux-menus/items/layouts.sh /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh Handling Pane'" "Move pane          -->" M "run-shell '/home/vscode/.tmux/plugins/tmux-menus/items/pane_move.sh'" "Resize pane        -->" R "run-shell '/home/vscode/.tmux/plugins/tmux-menus/items/pane_resize.sh'" "Pane history       -->" I "run-shell '/home/vscode/.tmux/plugins/tmux-menus/items/pane_history.sh'"  "" "Toggle zoom pane" z "resize-pane -Z  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Toggle mark current pane" m "select-pane -m  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Toggle synchronized panes" s "set-option -w synchronize-panes  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Set Pane Title" t "command-prompt -I '#T'  -p 'Title: '              'select-pane -T \"%%\"'  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Display pane numbers" \# "display-panes  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Display pane size" d "display-message             'Pane: #P size: #{pane_width}x#{pane_height}'  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "" "Last selected pane" l "last-pane  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Previous pane [in order]" p "select-pane -t :.-  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Next     pane [in order]" n "select-pane -t :.+  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "" "Respawn current pane" r "confirm-before -p 'respawn-pane #P? (y/n)' \"respawn-pane -k\"  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Kill current pane" x "confirm-before -p             'kill-pane #T (#P)? (y/n)' kill-pane  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh" "Kill all other panes" o "confirm-before -p             'Are you sure you want to kill all other panes? (y/n)'             \"kill-pane -a\"  ; run-shell /home/vscode/.tmux/plugins/tmux-menus/items/panes.sh"
